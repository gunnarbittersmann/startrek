					"episodeNumber": "2.3",
					"name": "Tomorrow and Tomorrow and Tomorrow",
					"datePublished": "2023-06-29",
					"director": {
						"@type": "Person",
						"@id": "https://bittersmann.de/startrek/people#AmandaRow",
						"name": "Amanda Row"
					},
					"author": {
