					"episodeNumber": "2.8",
					"name": "Under the Cloak of War",
					"datePublished": "2023-07-27",
					"director": {
						"@type": "Person",
						"@id": "https://bittersmann.de/startrek/people#JeffByrd",
						"name": "Jeff W. Byrd"
					},
					"author": {
						"@type": "Person",
