					"episodeNumber": "2.14",
					"name": "Cracked Mirror",
					"datePublished": "2024-07-01",
					"director": {
						"@type": "Person",
						"@id": "https://bittersmann.de/startrek/people#RuolinLi",
						"name": "Ruolin Li"
