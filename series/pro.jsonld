					"episodeNumber": "1.16",
					"name": "Preludes",
					"datePublished": "2022-12-01",
					"director": [
						{
							"@type": "Person",
							"@id": "https://bittersmann.de/startrek/people#SteveAhn",
							"name": "Steve Ahn"
						},
